DESTDIR=
DIST=	/nbsd

all:

install:
	diff src/libc/crypt.c ${DESTDIR}${DIST}/usr/src/lib/libc/gen/crypt.c
	diff src/libc/crypt.3 ${DESTDIR}${DIST}/usr/src/lib/libc/gen/crypt.3
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/login/Makefile*
	diff src/login/Makefile ${DESTDIR}${DIST}/usr/src/usr.bin/login
	#rm -rf ${DESTDIR}${DIST}/usr/src/bin/rcp
	diff -r src/rcp ${DESTDIR}${DIST}/usr/src/bin/rcp
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/passwd/Makefile*
	diff src/passwd/Makefile ${DESTDIR}${DIST}/usr/src/usr.bin/passwd
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/su/Makefile*
	diff src/su/Makefile ${DESTDIR}${DIST}/usr/src/usr.bin/su
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/rlogin
	diff -r src/rlogin ${DESTDIR}${DIST}/usr/src/usr.bin/rlogin
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/rsh
	diff -r src/rsh ${DESTDIR}${DIST}/usr/src/usr.bin/rsh
	#rm -rf ${DESTDIR}${DIST}/usr/src/libexec/rlogind
	diff -r src/rlogind ${DESTDIR}${DIST}/usr/src/libexec/rlogind
	#rm -rf ${DESTDIR}${DIST}/usr/src/libexec/rshd
	diff -r src/rshd ${DESTDIR}${DIST}/usr/src/libexec/rshd
	diff src/Makefile.libexec ${DESTDIR}${DIST}/usr/src/libexec/Makefile
	#rm -rf ${DESTDIR}${DIST}/usr/src/usr.bin/bdes
	diff -r src/bdes ${DESTDIR}${DIST}/usr/src/usr.bin/bdes
	diff -r src/kerberosIV/des ${DESTDIR}${DIST}/usr/src/kerberosIV/des

clean:
