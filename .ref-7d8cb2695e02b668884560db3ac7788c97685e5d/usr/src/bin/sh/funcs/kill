# Convert job names to process ids and then run /bin/kill.

kill() {
	local args x
	args=
	for x in "$@"
	do	case $x in
		%*)	x=`jobid "$x"` ;;
		esac
		args="$args $x"
	done
	/bin/kill $args
}
