#
# script to run off font catalog
#
TROFF=	csh /usr/ucb/vtroff -W
all:	titlepage intro chart apl basker bocklin bodoni chess clarendon\
	cm countdown cyrillic delegate fix gacham greek h19 hebrew\
	hershey meteor mona nonie oldenglish pip playbill shadow\
	script sign stare times ugramma
titlepage:	/tmp
	${TROFF} -3 bocklin titlepage
intro:	/tmp
	${TROFF} -ms intro.ms
chart:	/tmp
	tbl chart | ${TROFF} -ms
apl:	/tmp
	${TROFF} -1 apl apl
basker:	/tmp
	${TROFF} -F basker basker
bocklin:	/tmp
	${TROFF} -1 bocklin bocklin
bodoni:	/tmp
	${TROFF} -F bodoni bodoni
chess:	/tmp
	${TROFF} -1 chess chess
clarendon:	/tmp
	${TROFF} -3 clarendon clarendon
cm:	/tmp
	${TROFF} -F cm -ms cm
coronet:	/tmp
	${TROFF} -3 coronet coronet
countdown:	/tmp
	${TROFF} -1 countdown countdown
cyrillic:	/tmp
	${TROFF} -1 cyrillic cyrillic
delegate:	/tmp
	${TROFF} -F delegate delegate
fix:	/tmp
	${TROFF} -1 fix fix
gacham:	/tmp
	${TROFF} -F gacham gacham
greek:	/tmp
	${TROFF} -1 greek greek
h19:	/tmp
	${TROFF} -1 fix -2 h19 h19
hebrew:	/tmp
	${TROFF} -1 hebrew hebrew
hershey:	/tmp
	${TROFF} hershey
meteor:	/tmp
	${TROFF} -F meteor meteor
mona:	/tmp
	${TROFF} -1 mona mona
nonie:	/tmp
	${TROFF} -F nonie nonie
oldenglish:	/tmp
	${TROFF} -1 oldenglish oldenglish
pip:	/tmp
	${TROFF} -1 pip pip
playbill:	/tmp
	${TROFF} -1 playbill playbill
script:	/tmp
	${TROFF} -1 script script
shadow:	/tmp
	${TROFF} -1 shadow shadow
stare:	/tmp
	${TROFF} -F stare stare
sign:	/tmp
	${TROFF} -1 sign sign
times:	/tmp
	${TROFF} -F times times
ugramma:	/tmp
	${TROFF} -1 ugramma ugramma
