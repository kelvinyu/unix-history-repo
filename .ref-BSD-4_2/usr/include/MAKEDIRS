#! /bin/csh -f
#
#  Script to remove symbolic links to /sys, and create copies
#  of files in /sys instead.  For use at sites which cannot allow
#  /sys to be readable or do not keep it online.
#
umask 22
rm -f sys
mkdir sys
cp /sys/h/*.h sys
foreach dir (net netimp netinet netpup stand vax vaxif vaxmba vaxuba)
	rm -f $dir
	mkdir $dir
	cp /sys/$dir/*.h $dir
end
