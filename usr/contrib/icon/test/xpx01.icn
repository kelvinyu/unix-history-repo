record array(a,b,c,d,e,f,g)

procedure dummy(u,v,x,y,z)
   suspend u | v
   return x
end

procedure main()
   write("&trace := -1 ----> ",image(&trace := -1) | "none")
   write("every write(image(dummy{1,2,3,4})) ----> ",image(every write(image(dummy{1,2,3,4}))) | "none")
   write("every write(image(dummy{})) ----> ",image(every write(image(dummy{}))) | "none")
   write("image{1,2,3,4} ----> ",image(image{1,2,3,4}) | "none")
   write("image(put{1,2,3,4}) ----> ",image(image(put{1,2,3,4})) | "none")
   write("\"*\"{1,2,3,4} ----> ",image("*"{1,2,3,4}) | "none")
   write("image(\"|||\"([],[])) ----> ",image(image("|||"([],[]))) | "none")
   write("\"+\"(1,2) ----> ",image("+"(1,2)) | "none")
   write("\"+\"(1) ----> ",image("+"(1)) | "none")
   write("\"write\"(image(write)) ----> ",image("write"(image(write))) | "none")
   write("\"x\"(1) ----> ",image("x"(1)) | "none")
end
