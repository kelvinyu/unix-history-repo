record array(a,b,c,d,e,f,g)

procedure dummy(u,v,x,y,z)
   suspend u | v
   return x
end

procedure main()
   write("&trace := 2 ----> ",image(&trace := 2) | "none")
   write("dummy(1) ----> ",image(dummy(1)) | "none")
   write("every write(image(dummy(1,2.0,&null,[]))) ----> ",image(every write(image(dummy(1,2.0,&null,[])))) | "none")
   write("&trace ----> ",image(&trace) | "none")
   write("&trace := -1 ----> ",image(&trace := -1) | "none")
   write("every write(image(dummy(1,2.0,&null,[]))) ----> ",image(every write(image(dummy(1,2.0,&null,[])))) | "none")
   write("&trace ----> ",image(&trace) | "none")
   write("dummy() ----> ",image(dummy()) | "none")
end
