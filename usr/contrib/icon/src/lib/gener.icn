procedure star(s)
   suspend "" | (star(s) || !s)
end

procedure octal()
   suspend (0 to 3) || (0 to 7) || (0 to 7)
end

procedure hex()
   suspend !"0123456789abcdef" || !"0123456789abcdef"
end

procedure label(s,i)
   suspend s || i
   suspend s || (i +:= |1)
end
