rm -rf .tmp
mkdir .tmp
cd .tmp
for i in functions lib operators rt
do
        ar x ../../$i/Lib
done
rm __.SYMDEF
rtfiles="main.o init.o interp.o start.o pstart.o pstop.o"
for k in $rtfiles
do
  cp ../../iconx/$k .
done
ar cq ../rtlib *
cd ..
rm -rf .tmp
ranlib rtlib
lkfiles="builtin.o lcode.o glob.o llex.o lmem.o opcode.o lsym.o ilink.o"
for lf in $lkfiles
do
  ar cq linklib ../link/$lf
done
ranlib linklib
ed - icont.c <<EOF >/dev/null
g/config.h/d
w
q
EOF
