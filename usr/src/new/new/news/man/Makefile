	cp newsrc.5 $(DEST)/newsrc.n
	cp expire.8 $(DEST)/expire.n
	cp recnews.8 $(DEST)/recnews.n
	cp sendnews.8 $(DEST)/sendnews.n
	cp uurec.8 $(DEST)/uurec.n

