#	SEQIMAGE(2)
#
#	Image of Result Sequence
#
#	Ralph E. Griswold
#
#	Last modified 7/29/84
#

procedure Seqimage(a)
   local seq, result, i
   seq := ""
   i := @a[2]
   while result := image(@a[1]) do
      if *a[1] > \i then {
         seq ||:= ", ..."
         break
         }
      else seq ||:= ", " || result
   return "{" || seq[3:0] || "}" | "{}"
end
