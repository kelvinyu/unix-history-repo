link "../ilib/complex"

procedure main()
  x := list(6)
   while s1 := read() do {
      s2 := read() | stop()
      x[1] := strcpx(s1)
      x[2] := strcpx(s2)
      x[3] := cpxadd(x[1],x[2])
      x[4] := cpxdiv(x[1],x[2])
      x[5] := cpxmul(x[1],x[2])
      x[6] := cpxsub(x[1],x[2])
      every write(cpxstr(!x))
      }
end
