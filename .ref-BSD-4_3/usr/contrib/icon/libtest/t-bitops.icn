link "../ilib/bitops"

procedure main()
   while b1 := bitstring(read()) do {
      b2 := bitstring(read()) | break
      write(decimal(b1))
      write(decimal(b2))
      write("and=",and(b1,b2))
      write("not1=",neg(b1))
      write("not2=",not(b2))
      write("or=",or(b1,b2))
      write("exor=",exor(b1,b2))
      }
end

