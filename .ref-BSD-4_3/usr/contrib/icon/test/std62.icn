procedure main()
   write("36 % 7 ----> ",image(36 % 7) | "none")
   write("-36 % 7 ----> ",image(-36 % 7) | "none")
   write("36 % -7 ----> ",image(36 % -7) | "none")
   write("-36 % -7 ----> ",image(-36 % -7) | "none")
   write("36. % 7 ----> ",image(36. % 7) | "none")
   write("36 % 7. ----> ",image(36 % 7.) | "none")
   write("36. % 7. ----> ",image(36. % 7.) | "none")
   write("-36. % 7 ----> ",image(-36. % 7) | "none")
   write("36 % -7. ----> ",image(36 % -7.) | "none")
   write("-36. % -7. ----> ",image(-36. % -7.) | "none")
   write("2 % \"a\" ----> ",image(2 % "a") | "none")
end
