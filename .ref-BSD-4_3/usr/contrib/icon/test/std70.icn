procedure dummy(u,v,x,y,z)
   suspend u | v
   return x
end

procedure main()
   write("1 >= 1 ----> ",image(1 >= 1) | "none")
   write("1 >= 2 ----> ",image(1 >= 2) | "none")
   write("1 >= 0 ----> ",image(1 >= 0) | "none")
   write("-1 >= 0 ----> ",image(-1 >= 0) | "none")
   write("1 >= -2 ----> ",image(1 >= -2) | "none")
   write("-1 >= -0 ----> ",image(-1 >= -0) | "none")
   write("1. >= 1 ----> ",image(1. >= 1) | "none")
   write("1 >= 2. ----> ",image(1 >= 2.) | "none")
   write("1. >= 0. ----> ",image(1. >= 0.) | "none")
   write("-1 >= 0. ----> ",image(-1 >= 0.) | "none")
   write("1. >= -2 ----> ",image(1. >= -2) | "none")
   write("-1 >= -0. ----> ",image(-1 >= -0.) | "none")
   write("2 > \"a\" ----> ",image(2 > "a") | "none")
end
