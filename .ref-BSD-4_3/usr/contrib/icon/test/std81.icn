procedure main()
   write("c1 := 'abcde' ----> ",image(c1 := 'abcde') | "none")
   write("c2 := 'aeuoi' ----> ",image(c2 := 'aeuoi') | "none")
   write("c1 ++ c2 ----> ",image(c1 ++ c2) | "none")
   write("c1 -- c2 ----> ",image(c1 -- c2) | "none")
   write("c1 ** c2 ----> ",image(c1 ** c2) | "none")
   write("[] ** \"abc\" ----> ",image([] ** "abc") | "none")
end
