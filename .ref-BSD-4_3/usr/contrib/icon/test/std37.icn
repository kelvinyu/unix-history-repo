   write("t := table() ----> ",image(t := table()) | "none")
   write("every t[c := !12345678] := c ----> ",image(every t[c := !12345678] := c) | "none")
   write("every write(!t) ----> ",image(every write(!t)) | "none")
   write("x := array(1,2,3,4,5) ----> ",image(x := array(1,2,3,4,5)) | "none")
   write("every write(!x) ----> ",image(every write(!x)) | "none")
   write("pull[c] ----> ",image(pull[c]) | "none")
end
