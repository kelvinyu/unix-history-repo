procedure main()
   write("every write(!\"abcdef\") ----> ",image(every write(!"abcdef")) | "none")
   write("every write(![1,2,3,4,5]) ----> ",image(every write(![1,2,3,4,5])) | "none")
   write("every write(!![1,2,3,4,5]) ----> ",image(every write(!![1,2,3,4,5])) | "none")
   write("every write(!![1,\"ab\",[1,2,3],34]) ----> ",image(every write(!![1,"ab",[1,2,3],34])) | "none")
