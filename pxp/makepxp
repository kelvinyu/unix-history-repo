set time=3
cp /lib/libc.a .
ar d libc.a putchr.o
rm *.o
rm lib
gram
if ! -r ../nofloat goto floatcc
cc -c -O -f $1 *.c
goto contin
: floatcc
cc -c -O $1 *.c
: contin
as -o treen.o treen.s
as -o yycopy.o yycopy.s
version >Version.c
cc -c Version.c
ar r lib main.o yymain.o yyparse.o yyrecover.o y.tab.o fdec.o stat.o const.o type.o rval.o lval.o func.o proc.o
ar r lib *.o
load
rm *.o
